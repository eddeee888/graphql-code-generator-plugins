type Query
type Mutation
type Subscription

interface Error {
  error: ErrorType!
}

type PayloadError implements Error {
  error: ErrorType!
}

enum ErrorType {
  NOT_FOUND
  INPUT_VALIDATION_ERROR
  FORBIDDEN_ERROR
  UNEXPECTED_ERROR
}

input PaginationInput {
  recordsPerPage: Int
  page: Int
}

type PaginationResult {
  currentPage: Int!
  recordsPerPage: Int!
  totalPageCount: Int!
}

scalar DateTime
scalar SomeRandomScalar
